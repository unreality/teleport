version: v2
storage:
  type: directory
  path: /bot/storage
  symlinks: secure
  acls: try
outputs:
  - type: identity
    destination:
      type: directory
      path: /bot/output
    roles:
      - editor
    cluster: example.teleport.sh
  - type: identity
    destination:
      type: memory
  - type: identity
    destination:
      type: kubernetes_secret
      name: my-secret
services:
  - type: example
    message: llama
debug: true
auth_server: example.teleport.sh:443
certificate_ttl: 1m0s
renewal_interval: 30s
oneshot: true
fips: true
diag_addr: 127.0.0.1:1337
